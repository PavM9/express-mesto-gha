{
  "env": {
    "browser": false,
    "node": true,
    "mongo": true
},
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": ["error", {"allow": ["_id"] }],
    "object-curly-newline": ["error", { "ImportDeclaration": "always", "ExportDeclaration": "never" }],
    "linebreak-style": 0
  }
}